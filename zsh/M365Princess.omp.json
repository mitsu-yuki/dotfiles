{
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground_templates": [
            "{{ if .Segments.Python }}#047e84{{ else if .Segments.ssh }}#cc3802{{ else }}#9A348E{{ end }}"
          ],
          "background_templates": [
            "{{ if .Segments.Python }}#047e84{{ else if .Segments.ssh }}#cc3802{{ else }}#9A348E{{ end }}"
          ],
          "style": "diamond",
          "type": "text",
          "template": "<,transparent>\ue0b6"
        },
        {
          "background": "#047e84",
          "foreground": "#ffffff",
          "style": "powerline",
          "type": "python",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "fetch_version": false,
            "display_mode": "always",
            "fetch_virtual_env": true
          },
          "template": "{{ .Venv }} "
        },
        {
          "alias": "ssh",
          "background": "#cc3802",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "session",
          "template": "{{ if .SSHSession }}{{ if .Segments.Python}} \ueb01 {{ else }}\ueb01 {{ end }}{{ end }}"
        },
        {
          "background": "#9A348E",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "type": "session",
          "templates": [
            "{{ if .Segments.Python }} {{ .UserName }}@{{ .HostName }} {{ else if .Segments.ssh }} {{ .UserName }}@{{ .HostName }} {{ else }}{{ .UserName }}@{{ .HostName }} {{ end }}"
          ]
        },
        {
          "background": "#DA627D",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "style": "full"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "#FCA17D",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": false,
            "fetch_upstream_icon": true
          },
          "style": "powerline",
          "template": " \u279c ({{ .UpstreamIcon }}{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }}) ",
          "type": "git"
        },
        {
          "background": "#86BBD8",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "style": "powerline",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} ",
          "type": "node"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments" : [
        {
          "background": "#33658A",
          "foreground": "#ffffff",
          "leading_diamond": "\ue0b2",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " \u2665 {{ .CurrentDate | date .Format }} ",
          "trailing_diamond": "\ue0b4",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\u276f",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
