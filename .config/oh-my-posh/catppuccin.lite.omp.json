{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "flamingo": "#F2CDCD",
    "green": "#A6E3A1",
    "lavender": "#B4BEFE",
    "maroon": "#EBA0AC",
    "mauve": "#CBA6F7",
    "os": "#ACB0BE",
    "peach": "#FAB387",
    "pink": "#F5C2E7",
    "red": "#F38BA8",
    "rosewater": "#F5E0DC",
    "sapphire": "#74C7EC",
    "sky": "#89DCEB",
    "teal": "#94E2D5",
    "text": "#45475A",
    "yellow": "#F9e2af"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "os",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ abbrev 12 .HostName }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "session",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "home_icon": "~",
            "style": "full"
          },
          "template": " {{ .Path }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:pink",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ if and (ne .Env.BG_JOBS \"0\") (ne .Env.BG_JOBS \"\") }} \uf085 {{ .Env.BG_JOBS }}{{ end }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:peach",
          "type": "text",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "p:text",
          "background": "p:blue",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if eq .Code 0 }}\uf155{{ else }}\uf071  {{ reason .Code }}{{ end }}",
          "foreground": "p:teal",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
