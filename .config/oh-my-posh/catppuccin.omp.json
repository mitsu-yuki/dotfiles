{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "blue": "#89B4FA",
    "flamingo": "#F2CDCD",
    "green": "#A6E3A1",
    "lavender": "#B4BEFE",
    "maroon": "#EBA0AC",
    "mauve": "#CBA6F7",
    "os": "#ACB0BE",
    "peach": "#FAB387",
    "pink": "#F5C2E7",
    "red": "#F38BA8",
    "rosewater": "#F5E0DC",
    "sapphire": "#74C7EC",
    "sky": "#89DCEB",
    "teal": "#94E2D5",
    "text": "#45475A",
    "yellow": "#F9e2af"
  },
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "cache_duration": "none"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{.Icon}} ",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "os",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ .UserName }}@{{ abbrev 12 .HostName }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:blue",
          "type": "session",
          "style": "diamond"
        },
        {
          "options": {
            "cache_duration": "none",
            "home_icon": "~",
            "style": "full"
          },
          "template": " {{ .Path }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:pink",
          "type": "path",
          "style": "powerline"
        },
        {
          "options": {
            "branch_icon": "\ue725 ",
            "cache_duration": "none",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "template": "{{ .Templates }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:lavender",
          "type": "git",
          "style": "powerline",
          "templates": [
            "{{ if .UpstreamIcon }} {{ .UpstreamIcon }}{{ else }} \uf1d3 {{ end }}",
            " {{ .HEAD }}",
            "{{ if .Working.Changed }}",
            "{{ if .Staging.Added }} \uf055 {{ .Staging.Added }}{{ end }}",
            "{{ if .Staging.Modified }} \uf055 {{ .Staging.Modified }}{{ end }}",
            "{{ if .Working.Modified }} \uf06a {{ .Working.Modified }}{{ end }}",
            "{{ if .Working.Untracked }} \uf059 {{ .Working.Untracked }}{{ end }}",
            "{{ end }}",
            "{{ if .Ahead }} \uf01b {{ .Ahead }}{{ end }}",
            "{{ if .Behind }} \uf01a {{ .Behind }}{{ end }}",
            "{{ if gt .StashCount 0 }} \uf01c {{ .StashCount }}{{end}}"
          ]
        },
        {
          "options": {
            "cache_duration": "none"
          },
          "template": "{{ if and (ne .Env.BG_JOBS \"0\") (ne .Env.BG_JOBS \"\") }} \uf085 {{ .Env.BG_JOBS }}{{ end }}",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "background": "p:peach",
          "type": "text",
          "style": "powerline"
        }
      ],
      "newline": true
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "options": {
            "cache_duration": "none",
            "time_format": "15:04:05"
          },
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b4",
          "template": " {{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "p:text",
          "background": "p:blue",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "options": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "{{ if eq .Code 0 }}\uf155{{ else }}\uf071  {{ reason .Code }}{{ end }}",
          "foreground": "p:teal",
          "type": "exit",
          "style": "plain",
          "foreground_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 4,
  "final_space": true
}
