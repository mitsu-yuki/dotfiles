{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
        // mocha palette
        "os": "#ACB0BE",
        "rosewater": "#F5E0DC",
        "flamingo": "#F2CDCD",
        "pink": "#F5C2E7",
        "mauve": "#CBA6F7",
        "red": "#F38BA8",
        "maroon": "#EBA0AC",
        "peach": "#FAB387",
        "yellow": "#F9e2af",
        "green": "#A6E3A1",
        "teal": "#94E2D5",
        "sky": "#89DCEB",
        "sapphire": "#74C7EC",
        "blue": "#89B4FA",
        "lavender": "#B4BEFE",
        "text": "#45475A" // Surface 0
  },
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "leading_diamond": "\ue0b6",
          "style": "diamond",
          "template": "{{.Icon}} ",
          "type": "os"
        },
        {
          "background": "p:blue",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "diamond",
          "template": "{{ .UserName }}@{{ abbrev 12 .HostName }}",
          "type": "session"
        },
        {
          "background": "p:pink",
          "foreground": "p:text",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "background": "p:lavender",
          "foreground": "p:text",
          "style": "powerline",
          "properties": {
            "branch_icon": "\ue725 ",
            "cherry_pick_icon": "\ue29b ",
            "commit_icon": "\uf417 ",
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "merge_icon": "\ue727 ",
            "no_commits_icon": "\uf0c3 ",
            "rebase_icon": "\ue728 ",
            "revert_icon": "\uf0e2 ",
            "tag_icon": "\uf412 "
          },
          "templates": [
            "{{ if .UpstreamIcon }} {{ .UpstreamIcon }}{{ else }}  {{ end }}",
            " {{ .HEAD }}",
            "{{ if .Working.Changed }}",
              "{{ if .Staging.Added }}  {{ .Staging.Added }}{{ end }}",
              "{{ if .Staging.Modified }}  {{ .Staging.Modified }}{{ end }}",
              "{{ if .Working.Modified }}  {{ .Working.Modified }}{{ end }}",
              "{{ if .Working.Untracked }}  {{ .Working.Untracked }}{{ end }}",
            "{{ end }}",
            "{{ if .Ahead }}  {{ .Ahead }}{{ end }}",
            "{{ if .Behind }}  {{ .Behind }}{{ end }}",
            "{{ if gt .StashCount 0 }}  {{ .StashCount }}{{end}}"
          ],
          "powerline_symbol": "\ue0b4",
          "template": "{{ .Templates }}",
          "type": "git"
        },
        {
          "background": "p:peach",
          "foreground": "p:text",
          "powerline_symbol": "\ue0b4",
          "style": "powerline",
          "template": "{{ if and (ne .Env.BG_JOBS \"0\") (ne .Env.BG_JOBS \"\") }}  {{ .Env.BG_JOBS }}{{ end }}",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments" : [
        {
          "background": "p:blue",
          "foreground": "p:text",
          "leading_diamond": "",
          "properties": {
            "time_format": "15:04:05"
          },
          "style": "diamond",
          "template": " {{ .CurrentDate | date .Format }}  ",
          "trailing_diamond": "",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    // two line
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "p:teal",
          "foreground_templates": [
            "{{ if .Error }}p:red{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "{{ if eq .Code 0 }}{{ else }}  {{ reason .Code }}{{ end }}",
          "type": "exit"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
